{
  "name": "ptn-ninja",
  "version": "3.1.0",
  "description": "An editor and viewer for Portable Tak Notation",
  "productName": "PTN Ninja",
  "cordovaId": "org.gruppler.ptnninja.app",
  "author": "Craig Laparo <gruppler+github@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "dev": "quasar dev -m pwa",
    "build": "quasar build -m electron",
    "icons": "icongenie g -i app-icon.png --quality 12 --theme-color 607d8b --skip-trim",
    "deploy": "quasar build -m pwa && firebase deploy --only hosting:live",
    "dev-deploy": "quasar build -m pwa && firebase deploy --only hosting:dev",
    "rules-deploy": "firebase deploy --only firestore:rules",
    "functions-deploy": "firebase deploy --only functions"
  },
  "dependencies": {
    "@quasar/extras": "^1.8.1",
    "diff-match-patch": "^1.0.4",
    "firebase": "^7.14.3",
    "firebase-functions": "^3.6.1",
    "firebase-tools": "^7.16.2",
    "lodash": "^4.17.14",
    "lz-string": "^1.4.4",
    "marray": "^1.1.6",
    "quasar": "^1.11.3",
    "vue": "^2.6.11",
    "vue-i18n": "^8.0.0",
    "vue-moment": "^4.1.0",
    "vue-qriously": "^1.1.1",
    "vue-shortkey": "^3.1.7",
    "vue-smooth-reflow": "^0.1.12"
  },
  "devDependencies": {
    "@quasar/app": "^1.8.6",
    "@quasar/quasar-app-extension-qmarkdown": "^1.0.25",
    "@vue/eslint-config-prettier": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "devtron": "^1.4.0",
    "electron": "^4.2.12",
    "electron-debug": "^2.1.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^13.1.0",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^5.0.0",
    "firebase-admin": "^8.12.1"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
