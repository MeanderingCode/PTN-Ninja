{
  "name": "ptn-ninja",
  "version": "3.0.15",
  "description": "An editor and viewer for Portable Tak Notation",
  "productName": "PTN Ninja",
  "author": "Craig Laparo <gruppler+github@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "icons": "icongenie g -i app-icon.png --quality 12 --theme-color 607d8b --skip-trim",
    "dev": "quasar dev -m spa",
    "dev:pwa": "quasar dev -m pwa",
    "deploy": "quasar build -m pwa && firebase deploy --only hosting",
    "deploy:dev": "quasar build -m pwa && firebase deploy --only hosting:dev",
    "deploy:live": "quasar build -m pwa && firebase deploy --only hosting:live",
    "rules-deploy": "firebase deploy --only firestore:rules"
  },
  "dependencies": {
    "@quasar/extras": "^1.9.10",
    "aggregation": "^1.2.6",
    "core-js": "^3.0.0",
    "diff-match-patch": "^1.0.4",
    "firebase-functions": "^3.3.0",
    "firebase-tools": "^7.11.0",
    "lodash": "^4.17.14",
    "lz-string": "^1.4.4",
    "marray": "^1.1.6",
    "quasar": "^1.14.5",
    "vue": "^2.6.11",
    "vue-i18n": "^7.3.3",
    "vue-qriously": "^1.1.1",
    "vue-shortkey": "^3.1.7",
    "vue-smooth-reflow": "^0.1.12"
  },
  "devDependencies": {
    "@quasar/app": "^2.1.8",
    "@quasar/quasar-app-extension-qmarkdown": "^1.2.0",
    "@vue/eslint-config-prettier": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "devtron": "^1.4.0",
    "electron": "^4.2.12",
    "electron-debug": "^2.1.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^13.1.0",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^5.0.0",
    "workbox-webpack-plugin": "^5.0.0"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
